<xml>
	<object name="GenericI2CSensor" singleinstance="true" settings="false">
		<description>The raw data from the GenericI2C sensor with up to eight values, four ints and four floats</description>
		<field name="r0" units="-" type="int32" elements="1"/>
		<field name="r1" units="-" type="int32" elements="1"/>
		<field name="r2" units="-" type="int32" elements="1"/>
		<field name="r3" units="-" type="int32" elements="1"/>
		<field name="f0" units="-" type="float" elements="1"/>
		<field name="f1" units="-" type="float" elements="1"/>
		<field name="f2" units="-" type="float" elements="1"/>
		<field name="f3" units="-" type="float" elements="1"/>
		<access gcs="readwrite" flight="readwrite"/>
		<telemetrygcs acked="false" updatemode="manual" period="0"/>
		<telemetryflight acked="false" updatemode="periodic" period="500"/>
		<logging updatemode="manual" period="0"/>
	</object>
</xml>
