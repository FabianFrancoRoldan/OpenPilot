<xml>
    <object name="DCMStatus" singleinstance="true" settings="false">
        <description>The DCM status</description>
        <field name="rawGyros" units="deg/s" type="float" elements="3"/>
        <field name="omegaCorrP" units="deg/s" type="float" elements="3"/>
        <field name="omegaCorrI" units="deg/s" type="float" elements="3"/>
        <field name="normOmegaScalar" units="deg/s" type="float" elements="1"/>
        <access gcs="readwrite" flight="readwrite"/>
        <telemetrygcs acked="false" updatemode="manual" period="0"/>
        <telemetryflight acked="false" updatemode="periodic" period="500"/>
        <logging updatemode="manual" period="0"/>
    </object>
</xml>
