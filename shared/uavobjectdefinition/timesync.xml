<xml>
    <object name="TimeSync" singleinstance="true" settings="false">
        <description>Used to synchronize time when running in UAVTalk Debug mode.</description>
        <field name="dummyData" units="" type="uint8" elements="1"/>
        <access gcs="readwrite" flight="readwrite"/>
        <telemetrygcs acked="true" updatemode="periodic" period="5000"/>
        <telemetryflight acked="true" updatemode="periodic" period="5000"/>
        <logging updatemode="never" period="0"/>
    </object>
</xml>
